      <span .action .edit .glyphicon .glyphicon-edit>
      <span .action .delete .glyphicon .glyphicon-trash>
  <span #edit_name>
    <input type=text name=name value=#{M.flightName flight}>
    <span>
      <span .action .save .glyphicon .glyphicon-ok>

<div #player>

<div #map>

